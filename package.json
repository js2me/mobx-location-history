{
  "name": "mobx-location-history",
  "version": "2.3.1",
  "scripts": {
    "clean": "rimraf dist",
    "check": "eslint . --fix",
    "prebuild": "npm run clean && npm run check",
    "build:watch": "pnpm build && nodemon --delay 0.5 --watch src --ext ts,tsx --exec \"pnpm build\"",
    "build": "tsc && node ./post-build.mjs",
    "pub:patch": "PUBLISH=true PUBLISH_VERSION=patch pnpm run build",
    "pub:minor": "PUBLISH=true PUBLISH_VERSION=minor pnpm run build",
    "pub:major": "PUBLISH=true PUBLISH_VERSION=major pnpm run build",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "dev": "pnpm test:watch"
  },
  "keywords": [
    "mobx",
    "history",
    "location",
    "observable"
  ],
  "author": "js2me",
  "license": "MIT",
  "description": "Browser Location and History charged by MobX",
  "type": "module",
  "bugs": {
    "url": "https://github.com/js2me/mobx-location-history/issues"
  },
  "homepage": "https://github.com/js2me/mobx-location-history",
  "repository": {
    "type": "git",
    "url": "git://github.com/js2me/mobx-location-history"
  },
  "dependencies": {
    "linked-abort-controller": "^1.1.0",
    "yummies": "^3.2.3"
  },
  "peerDependencies": {
    "mobx": "^6.13.3"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react-swc": "^3.9.0",
    "@vitest/coverage-istanbul": "^3.1.2",
    "eslint": "^8.57.1",
    "js2me-eslint-config": "^1.0.7",
    "js2me-exports-post-build-script": "^2.0.18",
    "jsdom": "^26.1.0",
    "nodemon": "^3.1.10",
    "typescript": "^5.8.3",
    "vitest": "^3.1.2"
  }
}